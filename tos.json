{
    "type": "AdaptiveCard",
    "$schema": "https://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "body": [
        {
            "type": "Container",
            "items": [
                {
                    "type": "ColumnSet",
                    "columns": [
                        {
                            "type": "Column",
                            "width": "auto",
                            "items": [
                                {
                                    "type": "Image",
                                    "url": "https://mad.kiwi/madhaus/mike.png"
                                }
                            ]
                        },
                        {
                            "type": "Column",
                            "width": "stretch",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "wrap": true,
                                    "style": "heading",
                                    "text": "Madkiwi's Madhaus",
                                    "fontType": "Monospace",
                                    "color": "Dark"
                                },
                                {
                                    "type": "TextBlock",
                                    "text": "This server collects data to create a player account. This information is stored in a database and will NEVER be sold or used outside of this project.",
                                    "wrap": true,
                                    "color": "Dark"
                                },
                                {
                                    "type": "TextBlock",
                                    "text": "IF YOU DO NOT WANT  THIS. DO NOT PROCEED. ",
                                    "wrap": true,
                                    "color": "Dark"
                                },
                                {
                                    "type": "TextBlock",
                                    "text": "Close this window NOW...",
                                    "wrap": true,
                                    "color": "Dark"
                                },
                                {
                                    "type": "TextBlock",
                                    "text": "bob",
                                    "wrap": true,
                                    "color": "Dark"
                                }
                            ],
                            "backgroundImage": {
                                "url": "https://madkiwi.ca/purplemonkeydishwasher/media/img/Blank720.png"
                            }
                        }
                    ]
                }
            ]
        }
    ],
    "backgroundImage": {
        "url": "https://madkiwi.ca/purplemonkeydishwasher/media/img/smallbg.png"
    }
}